<UserControl x:Class="CoinManager.Component.TitreValeurCrypto"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CoinManager.Component"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="600">
    <StackPanel Orientation="Horizontal">
        <Label Content="Symbol" Width="100"></Label>
        <Label Content="Quantité" Width="100"></Label>
        <Label Content="Valeur" Width="100"></Label>
        <Label Content="Moyenne" Width="100"></Label>
        <Label Content="Cours" Width="100"></Label>
        <Label Content="Diff" Width="100"></Label>
    </StackPanel>
</UserControl>
