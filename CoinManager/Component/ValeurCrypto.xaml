<UserControl x:Class="CoinManager.Component.ValeurCrypto"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CoinManager.Component"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="500">
    <StackPanel Orientation="Horizontal">
        <Label Content="{Binding Symbol}" Width="100"></Label>
        <Label Content="{Binding NombreStr}" Width="100"></Label>
        <Label Content="{Binding ValeurStr}" Width="100"></Label>
        <Label Content="{Binding MoyenneStr}" Width="100"></Label>
        <Label Content="{Binding CoursStr}" Width="100"></Label>
    </StackPanel>
</UserControl>
